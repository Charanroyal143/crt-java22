package pack1;
class First
{
	private int a=10;
	int b=20;
	protected int c=30;
	public int d=40;
	void show()
	{
		System.out.println("private a="+a);
        System.out.println("int b="+a);
        System.out.println("protected c="+a);
        System.out.println("public c="+a);
    }
	  public static void main(String[] args)
	{
		  First f1=new First();
		  f1.show();
	}
}

   